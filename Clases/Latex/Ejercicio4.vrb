\frametitle{Tercera diapositiva}
\begin{verbatim}
	x = 10.5; y = 1.0/3; z = 15.3
	# Esta es otra forma de escribir las variables: x,y,z=10.5,1.0/3,15.3
	H = '''
	El punto en R3 es:
	(x,y,z)=(%.2f,%g,%G)
	''' % (x,y,z)
	print H

	G = '''
	El punto en R3 es:
	(x,y,z)=({laX:.2f},{laY:g},{laZ:G}
	'''.format(laX=x,laY=y,laZ=z)
	print G
	\end{verbatim}
